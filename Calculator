x=float(input("Enter the first number: "))
y=float(input("Enter the second number: "))
print("\n\n")

def division(x,y):

    if y==0:
        print("Division: Can't divide with zero")
    else:
        print("Division:" ,x/y)
        


print("Addition:" ,x+y)
print("Subtraction:" , x-y )
print("Multiplication:" ,x*y)
division(x,y)

